<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="jp.tm.touchapp">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TouchApp"
        tools:targetApi="31">

        <!--
             TODO: Before you run your application, you need a Google Maps API key.

             To get one, follow the directions here:

                https://developers.google.com/maps/documentation/android-sdk/get-api-key

             Once you have your API key (it starts with "AIza"), define a new property in your
             project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
             "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="Enter_You_App_Key" />

        <activity
            android:name=".MapsActivity"
            android:exported="false"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".FirstCountDownTimerActivity"
            android:exported="false" />
        <activity
            android:name=".FirstMediaPlayerActivity"
            android:exported="false" />
        <activity
            android:name=".FirstTodoActivity"
            android:exported="false" />
        <activity
            android:name=".ListView2Activity"
            android:exported="false" />
        <activity
            android:name=".ListViewActivity"
            android:exported="false" />
        <activity
            android:name=".CatchCopyActivity"
            android:exported="false" />
        <activity
            android:name=".ResultActivity"
            android:exported="false" />
        <activity
            android:name=".MultiQuizActivity"
            android:exported="false" />
        <activity
            android:name=".SimpleQuizActivity"
            android:exported="false" />
        <activity
            android:name=".FiveScoreActivity"
            android:exported="false" />
        <activity
            android:name=".GameClearActivity"
            android:exported="false" />
        <activity
            android:name=".GameOverActivity"
            android:exported="false" />
        <activity
            android:name=".FirstPutExtraActivity"
            android:exported="false" />
        <activity
            android:name=".SecondPutExtraActivity"
            android:exported="false" />
        <activity
            android:name=".FirstActivity"
            android:exported="false" />
        <activity
            android:name=".SecondActivity"
            android:exported="false" />
        <activity
            android:name=".NekoPunchActivity"
            android:exported="false" />
        <activity
            android:name=".CalucurateActivity"
            android:exported="false" />
        <activity
            android:name=".DivinationActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- SDK が28以上のときにこの記載がGoogle Mapのために必要・・・？-->
        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>